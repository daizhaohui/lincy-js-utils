{
  "name": "@lincy-js/utils",
  "version": "1.0.0",
  "description": "通用js工具库",
  "main": "src/index.ts",
  "scripts": {
    "dev": "export NODE_ENV=development && gulp development",
    "prod": "export NODE_ENV=production && gulp production",
    "pack": "export NODE_ENV=production && gulp pack",
    "test": "jest",
    "test-c": "jest --coverage",
    "lint": "npx tslint \"src/**/*.ts\" --force"
  },
  "author": "daizhaohui",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "@babel/runtime-corejs3": "^7.13.7",
    "core-js": "3.9.1",
    "dayjs": "^1.10.7",
    "gulp-run-command": "^0.0.10",
    "js-md5": "^0.7.3",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/js-md5": "^0.4.3",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__test__/.*\\.(ts|tsx|js)$"
  }
}
